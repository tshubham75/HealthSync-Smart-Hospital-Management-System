<%= form_with(model: @room, url: @room.persisted? ? admin_room_path(@room) : admin_rooms_path, local: true) do |f| %>
  <div>
    <%= f.label :department_id %>
    <%= f.text_field :department_id %>
  </div>

  <div>
    <%= f.label :room_number %>
    <%= f.text_field :room_number %>
  </div>

  <div>
    <%= f.label :user_id %>
    <%= f.text_field :user_id %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>
